int a=500;
int b;
int c[10];

void main() {
	int x;
	int y =10;
	int z[3];
	x = 2;
	x = y;
	x = a;
	c[0] = x;
	z[0] = 5;
	z[1] = 10;
	z[2] = 15;
	if(x == y) {
		++x;
	}
	if ( x != 2 ) {
		--y;
	} else {
		y = z[0] + 1;
	}
	while ( x >= 0 ) {
		x = x - 1;
	}
	foo(x, z, y);
	return 3;
}

int foo( int a, int b[], int c) {
	int z;
	int zz;
	z  = a;
	zz = b[0] + c;
	
	return zz - z;
}
